<!----Contenido-->


<div class="row mt-5 mb-5" style="min-height: 1000px;">
  <div class="col-1"></div>
  <div class="col">
<div class="row">

<div class="col-3 p-2" >
  <div class="d-block"  >
      <button class="btn btn_dark font-weight-bold  w-100 mb-3" type="button" id="cart_cine_drop"  >
        Lugar<i class="fas fa-chevron-down float-right mt-1" style="right: 0px;"></i>
      </button>
      <div class="down_cine    box_filtro " id="down_cine">
        <select class="custom-select my-1 mr-sm-2 btn_border_dark border-dark decorated" id="inlineFormCustomSelectPref1">
          <option selected>Escoger lugar...</option>
          <option value="1">Coahuila</option>
          <option value="2">Durango</option>
          <option value="3">Edo de México </option>
          <option value="4">Nuevo León</option>
          <option value="5">Guanajuato</option>
          <option value="6">Hidalgo</option>
          <option value="7">Jalisco</option>
          <option value="8">Morelos</option>
          <option value="9">Nayarit</option>
        </select>
        <select class="custom-select my-1 mr-sm-2 btn_border_dark border-dark decorated" id="inlineFormCustomSelectPref1">
          <option selected>Escoger cine...</option>
          <option value="1">Cinépolis Garza Sada</option>
          <option value="2">Cinemex Cumbres</option>>
        </select>
      </div>
      <button class="btn btn_dark font-weight-bold  w-100 mb-3" type="button" id="cart_formato_drop"  >
        Formato<i class="fas fa-chevron-down float-right mt-1" style="right: 0px;"></i>
      </button>
      <div class="down_cine   box_filtro  " id="down_formato">
        <div class="form-check">
          <input class="form-check-input" type="checkbox" value="" id="check1">
          <label class="form-check-label" for="check1">
            Video 2D
          </label>
        </div>
        <div class="form-check">
          <input class="form-check-input" type="checkbox" value="" id="check2">
          <label class="form-check-label" for="check2">
            Video 3D
          </label>
        </div>
      </div>
      <button class="btn btn_dark font-weight-bold  w-100 mb-3" type="button" id="cart_idioma_drop"  >
        Idioma<i class="fas fa-chevron-down float-right mt-1" style="right: 0px;"></i>
      </button>
      <div class="down_cine    box_filtro " id="down_idioma">
        <div class="form-check">
          <input class="form-check-input" type="radio" name="exampleRadios" id="radios1" value="option1" checked>
          <label class="form-check-label" for="radios1">
            Español
          </label>
        </div><div class="form-check">
          <input class="form-check-input" type="radio" name="exampleRadios" id="radios2" value="option1" checked>
          <label class="form-check-label" for="radios2">
            Inglés
          </label>
        </div><div class="form-check">
          <input class="form-check-input" type="radio" name="exampleRadios" id="radios3" value="option1" checked>
          <label class="form-check-label" for="radios3">
            Subtitulada
          </label>
        </div>
      </div>
      <button class="btn btn_dark font-weight-bold  w-100 mb-3" type="button" id="cart_horario_drop"  >
        Horario<i class="fas fa-chevron-down float-right mt-1" style="right: 0px;"></i>
      </button>
      <div class="down_cine  p-2  box_filtro " id="down_horario">
        <p  class=" ">
          <input class="text-center w-100" type="text" id="amount" readonly style="border:0; color:#ffd100; font-weight:bold;background-color: #f5f5f5;">
        </p>
        <div id="slider-range" ></div>
      </div>
      <button class="btn btn_dark font-weight-bold  w-100 mb-3" type="button" id="cart_calendario_drop"  >
        Día<i class="fas fa-chevron-down float-right mt-1" style="right: 0px;"></i>
      </button>
      <div class="down_cine    box_filtro" id="down_calendario">
        <input type="text" id="datetimepicker" class="  w-100" placeholder="0000/00/00" style="border-radius: .25rem;border: 1px solid #ced4da;    padding: .375rem 1.75rem .375rem .75rem; "/>

      </div>

      
    </div>
</div>
<div class="col-7 pl-5 pr-5 pt-2">
  <%  if(carteleras == ""){ %>
    <p>No se encontró cartelera.</p>
    <% } %>
  <%  if(carteleras != ""){ %>
    <%  for(var i = 0; i < carteleras.length ; i++){ %>
    <table class="table table-striped mb-3">
      <thead>
        <tr>
          
          <th scope="col" class="bg_cartelera">  <%= carteleras[i].cine + " - " +  carteleras[i].estado  %> <i class="fas fa-info-circle float-right pt-1"  style="right: 3px;" id="info_cine<%= i%>"data-toggle="dropdown"></i>
            <div class="dropdown-menu" aria-labelledby="info_cine<%= i%>">
              <h4>Direccion</h4>
              <p>Este cine se encuentra si vas ...</p>
              <div class="w-100 map " id="map_cine<%= i%>">
                </div>
                <script>
                  var map_cine = new GMaps({
                    el: '#map_cine<%= i%>',
                    lat: -12.043333,
                    lng: -77.028333
                  });
                </script>
            </div>
          </th>
        
        </tr>
        
     
      </thead>
     
      <tbody>
        <tr>
          <th scope="row" class="p-5"> 
            <div class="row">
              <div class="col-3">
                <img src="/img/<%= carteleras[i].file %>" alt="" class="img_cartelera">
           
              </div>
              <div class="col-9"> 
                <div class=" w-100 h-100 " >
                  <h3> <%= carteleras[i].pelicula %></h3>
                  <h5><%= carteleras[i].idioma %></h5>
                  <a type="button" target="blank" href="<%= carteleras[i].link %>" class="btn btn_sensa_priory btn-sm mb-1"><%= carteleras[i].hora + " hrs" %></a>
                 
                  
                  <p><a type="button" class="btn btn-dark float-right" style="right: 3px;">Ver avance</a></p>
              </div>
            </div>
            </div>
          </th>
        </tr>
        
      </tbody>
    </table>
<%  } %>
    <% } %>
</div>
<div class="col-2 p-2 bg-secondary text-center border-dark text-dark w-100 " style="height: 700px;" >
Anuncio
</div>


</div>
  </div>
  <div class="col-1"></div>
</div>


